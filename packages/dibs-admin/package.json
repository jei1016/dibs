{
  "name": "@bearcove/dibs-admin",
  "version": "0.1.0",
  "type": "module",
  "svelte": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "svelte": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./styles": "./dist/styles/app.css",
    "./styles/*": "./dist/styles/*",
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts",
      "default": "./dist/types/*.js"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "default": "./dist/lib/*.js"
    },
    "./lib/ui": {
      "svelte": "./dist/lib/ui/index.js",
      "types": "./dist/lib/ui/index.d.ts",
      "default": "./dist/lib/ui/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "svelte-package -i src -o dist",
    "prepare": "pnpm run build",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "dependencies": {
    "@bearcove/sextant": "github:bearcove/sextant",
    "@bearcove/squel-service": "workspace:*",
    "@codemirror/commands": "^6.8.1",
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-html": "^6.4.11",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/lang-markdown": "^6.5.0",
    "@codemirror/language": "^6.11.1",
    "@codemirror/state": "^6.5.4",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.39.11",
    "@replit/codemirror-vim": "^6.3.0",
    "bits-ui": "^2.15.4",
    "codemirror": "^6.0.2",
    "phosphor-svelte": "^3.0.0"
  },
  "devDependencies": {
    "@internationalized/date": "^3.10.1",
    "@sveltejs/package": "^2.3.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "svelte": "^5.16.2",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
